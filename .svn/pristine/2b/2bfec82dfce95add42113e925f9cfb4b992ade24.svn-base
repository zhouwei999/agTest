                <div nz-row nzType="flex" nzAlign="middle">
                    <nz-form-item class="widthStyle"> 
                        <nz-form-control >
                            <label nz-col nz-checkbox formControlName="cchpisCheck" (ngModelChange)="requiredChange(['cchpparam1','cchpparam2','cchpparam3'],$event)" [(ngModel)]="cchpisCheck"><span class="labelTitle">冷热电三联供(CCHP)</span></label>                  
                        </nz-form-control>
                    </nz-form-item>
                    <nz-form-item nzFlex class="widthStyle2">
                        <nz-form-label [nzNoColon]="true">热效率</nz-form-label>   
                        <nz-form-control nzErrorTip="请输入热效率！">
                            <nz-input-number [nzMin]=0 [nzMax]=1 nz-input formControlName="cchpparam1" [nzDisabled]="!cchpisCheck"></nz-input-number>
                        </nz-form-control>
                    </nz-form-item> 
                    <nz-form-item nzFlex class="widthStyle2"> 
                        <nz-form-label [nzNoColon]="true">发电效率</nz-form-label>         
                        <nz-form-control nzErrorTip="请输入发电效率！">
                            <nz-input-number [nzMin]=0 [nzMax]=1 nz-input formControlName="cchpparam2" [nzDisabled]="!cchpisCheck"></nz-input-number>
                        </nz-form-control>
                    </nz-form-item> 

.content{
    height: 695px;
    background: #fff;
    width: 100%;
    padding: 25px 40px 10px 50px;
    .site{
        float: left;
        line-height: 33px;
        margin-right: 18px;
    }
    .search{
        float: left;
        width: 260px;
        position: relative;
        input{
            height: 33px;
        }
        button{
            height: 33px;
        }
        .placeList{
            position: absolute;
            left: 0px;
            top: 32px;
            border: 1px solid #ccc;
            padding: 0px;
            width: 196px;
            z-index: 10;
            background: #fff;
            border-radius: 4px;
            height: 200px;
            // min-height: 50px;
            overflow-y: scroll;
        }
        .placeList li{
            line-height: 30px;
            padding-left: 15px;
            cursor: pointer;
        }
        .placeList li:hover{
            background: #f5f5f5;
        }
    }
}
.gutter-box {
    padding: 5px 0;
}
.gutter-example{
    margin-top: 20px;
}
#map{
    border: 1px solid #ccc;
    width:100%;
    height: 550px;
}
.chart{
    border: 1px solid #ccc;
    width:100%;
    height: 550px;
    .header{
        height: 50px;
        background: #f2f2f2;
        line-height: 50px;
        padding: 0 20px 0 10px;
        button{
            margin-top: 9px;
            margin-left:15px;
        }
        .typical{
            font-weight: bold;
            font-size: 16px;
        }
        .download{
            cursor: pointer;
            color: #02b980;
        }
    }
    .seasonContent{
        padding: 15px 10px;
        [nz-button] {
            margin-bottom: 12px;
            width: 100px;
            height: 30px;
        }
        
        nz-button-group {
            margin-bottom: 8px;
            margin-right: 8px;
        }
        .chartTable{
            height: 430px;
            overflow-x: hidden;
            overflow-y: auto;
            .chartHeat{
                height: 20px;
                width: 100%;
                line-height: 20px;
            }
            #lineChart{
                width: 100%;
                height:250px;
            }
        }
        .windTable{
            .windTablehead{
                height: 35px;
                background: #f2f2f2;
                line-height: 35px;
                padding: 0 10px;
                margin-top: 10px;
            }
        }
        .editable-cell {
            position: relative;
          }
    
          .editable-cell-value-wrap {
            padding: 5px 12px;
            cursor: pointer;
          }
    
          .editable-row:hover .editable-cell-value-wrap {
            border: 1px solid #d9d9d9;
            border-radius: 4px;
            padding: 4px 11px;
          }
    }
}

::ng-deep .ant-tabs.ant-tabs-card .ant-tabs-card-bar .ant-tabs-tab {
    border: 0;
    border-top: 1px solid #ccc;
    border-right: 1px solid #ccc;
    border-bottom: 1px solid #ccc;
    border-radius: 0;
}

::ng-deep .ant-tabs-tab-active{
    border-bottom: 0 !important; 
}

::ng-deep .ant-tabs.ant-tabs-card .ant-tabs-card-bar .ant-tabs-tab{
    margin-right: 0 ;
}

:host ::ng-deep .ant-tabs-bar{
    margin:0;
    border:0;
    border-left: 1px solid #ccc;
}

::ng-deep .ant-tabs .ant-tabs-top-content, .ant-tabs .ant-tabs-bottom-content{
    border: 1px solid #ccc;
}

